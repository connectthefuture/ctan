@Article{leinster:rst,
  author =       {Tom Leinster},
  title =        {Rethinking Set Theory},
  doi =          {10.4169/amer.math.monthly.121.05.403},
  abstract =     {Abstract Mathematicians manipulate sets with confidence almost every
                  day, rarely making mistakes. Few of us, however, could accurately quote
                  what are often referred to as 'the' axioms of set theory. This suggests
                  that we all carry around with us, perhaps subconsciously, a reliable
                  body of operating principles for manipulating sets. What if we were to
                  take some of those principles and adopt them as our axioms instead? The
                  message of this article is that this can be done, in a simple, practical
                  way (due to Lawvere). The resulting axioms are ten thoroughly mundane
                  statements about sets.},
  journaltitle = {The American Mathematical Monthly},
  number =       5,
  pages =        {403-415},
  publisher =    {Mathematical Association of America},
  volume =       121,
  year =         2014,
  language =     {english},
  langid =       {english},
}

