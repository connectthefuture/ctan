@Article{shulman:stct,
  author =       {Michael A. Shulman},
  title =        {Set theory for category theory},
  date =         {2008-10-07},
  _year =        2008,
  language =     {english},
  langid =       {english},
  eprinttype =   {arxiv},
  eprint =       {0810.1279v2},
  eprintclass =  {math.CT},
  annote =       {Questions of set-theoretic size play an essential role in category
                  theory, especially the distinction between sets and proper classes (or
                  small sets and large sets). There are many different ways to formalize
                  this, and which choice is made can have noticeable effects on what
                  categorical constructions are permissible. In this expository paper we
                  summarize and compare a number of such "set-theoretic foundations for
                  category theory," and describe their implications for the everyday use
                  of category theory. We assume the reader has some basic knowledge of
                  category theory, but little or no prior experience with formal logic or
                  set theory.},
}

