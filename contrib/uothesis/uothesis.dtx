		\hskip -\leftskip
     		{#4}\nobreak
    		\leaders\hbox{$\m@th
        			\mkern \@dotsep mu\hbox{.}\mkern
			\@dotsep
        			mu$}\hfill
     		\nobreak
     		\hb@xt@\@pnumwidth{\hfil\normalfont
		\normalcolor #5}
     		\par}
\fi}
